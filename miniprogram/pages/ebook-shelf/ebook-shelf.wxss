.ebook-shelf-container{
  background-color: #fafafa;
}
.add-ebook{
  width: 220rpx;
  height: 300rpx;
  border: 3px solid #ddd;
  position: relative;
  z-index: 99;
}
.add-position{
  position: absolute;
  top: 50%;
  left:50%;
  transform: translate(-50% , -50%);
}